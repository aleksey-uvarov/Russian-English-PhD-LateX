	langid = {english},
	eprinttype = {arxiv},
	eprint = {2101.08448},
	keywords = {Quantum Physics, Condensed Matter - Statistical Mechanics, Computer Science - Machine Learning, Computer Science - Artificial Intelligence},
	file = {Bharti и др. - 2021 - Noisy intermediate-scale quantum (NISQ) algorithms.pdf:/home/alexey/Yandex.Disk/Research/Literature/Bharti и др. - 2021 - Noisy intermediate-scale quantum (NISQ) algorithms.pdf:application/pdf}
