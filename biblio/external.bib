	eprinttype = {arxiv},
	eprint = {0905.0887},
	keywords = {Quantum Physics},
	file = {Lanyon и др. - 2010 - Towards Quantum Chemistry on a Quantum Computer.pdf:/home/alexey/Yandex.Disk/Research/Literature/Lanyon и др. - 2010 - Towards Quantum Chemistry on a Quantum Computer.pdf:application/pdf}
