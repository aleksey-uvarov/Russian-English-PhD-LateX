	keywords = {Mathematics - Optimization and Control, Quantum Physics},
	file = {Harrow и Napp - 2019 - Low-depth gradient measurements can improve conver.pdf:/home/alexey/Yandex.Disk/Research/Literature/Harrow и Napp - 2019 - Low-depth gradient measurements can improve conver.pdf:application/pdf}
}

@article{kempe_complexity_2006,
