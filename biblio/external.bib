	langid = {english},
	eprinttype = {arxiv},
	eprint = {2001.00550},
	keywords = {Quantum Physics, Computer Science - Machine Learning},
	file = {Cerezo и др. - 2020 - Cost-Function-Dependent Barren Plateaus in Shallow.pdf:/home/alexey/Yandex.Disk/Research/Literature/Cerezo и др. - 2020 - Cost-Function-Dependent Barren Plateaus in Shallow.pdf:application/pdf;Cerezo и др. - 2020 - Cost-Function-Dependent Barren Plateaus in Shallow.pdf:/home/alexey/Yandex.Disk/Research/Literature/Cerezo и др. - 2020 - Cost-Function-Dependent Barren Plateaus in Shallow 2.pdf:application/pdf}
